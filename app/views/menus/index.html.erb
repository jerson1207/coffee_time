<h1>Menu</h1>
<ul>
  <% @menus.each do |menu| %>
    <li>
      <%= link_to menu.title, menu %>
    </li>
    <li>
      <%= menu.description%>
    </li>
    <li>
      <%= menu.price%>
    </li>
    <li><%= link_to "Edit", edit_menu_path(menu) %></li>
  <% end %>
</ul>

<%= link_to "New menu", new_menu_path %>